<% provide(:title, @photo.title) %>

<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.user_name %>
      </h1>
    </section>
  </aside>
  <div class="col-md-8">
    <ol class="photos">
      <%= render @photo %>
    </ol>
  </div>
</div>
